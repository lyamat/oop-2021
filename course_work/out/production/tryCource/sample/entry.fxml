<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="229.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EntryController">
   <children>
      <ImageView fx:id="gradient3ImageView" fitHeight="335.0" fitWidth="382.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/gradient3.jpg" />
         </image>
      </ImageView>
      <Button fx:id="signInButton" layoutX="112.0" layoutY="32.0" mnemonicParsing="false" onAction="#singInButtonOnAction" prefHeight="46.0" prefWidth="158.0" style="-fx-background-color: transparent;" text="Sing in">
         <font>
            <Font name="Tahoma" size="35.0" />
         </font>
      </Button>
      <Button fx:id="signUpButton" layoutX="106.0" layoutY="129.0" mnemonicParsing="false" onAction="#singUpButtonOnAction" prefHeight="33.0" prefWidth="170.0" style="-fx-background-color: transparent;" text="Sing up">
         <font>
            <Font name="Tahoma" size="35.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="29.0" prefWidth="65.0" style="-fx-background-color: transparent;" text="EXIT">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
