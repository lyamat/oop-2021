<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TripController">
   <children>
      <TableView fx:id="tripTableView" layoutX="232.0" layoutY="53.0" prefHeight="200.0" prefWidth="305.0">
        <columns>
          <TableColumn fx:id="tripIDColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tripCountryColumn" prefWidth="125.60003662109375" text="Country" />
            <TableColumn fx:id="tripCostColumn" prefWidth="103.99993896484375" text="Cost" />
        </columns>
      </TableView>
      <TextField fx:id="countryTextField" layoutX="32.0" layoutY="53.0" promptText="Country" />
      <TextField fx:id="costTextField" layoutX="32.0" layoutY="96.0" promptText="Cost" />
      <Label fx:id="addedTripLabel" layoutX="13.0" layoutY="280.0" prefHeight="26.0" prefWidth="291.0" />
      <Button fx:id="addTripButton" layoutX="31.0" layoutY="153.0" mnemonicParsing="false" onAction="#addTripButtonOnAction" prefHeight="26.0" prefWidth="150.0" text="Add" />
      <Button fx:id="signOutButton" layoutX="31.0" layoutY="200.0" mnemonicParsing="false" onAction="#signOutButtonOnAction" prefHeight="26.0" prefWidth="150.0" text="Sign out" />
   </children>
</AnchorPane>
